{
  "name": "wafflebot",
  "version": "2.0.0",
  "description": "An IRC Wafflebot",
  "bin": {
    "wafflebot": "build/bin/wafflebot"
  },
  "dependencies": {
    "commander": "^2.9.0",
    "irc": "^0.4.1",
    "log4js": "^0.6.37",
    "moment": "^2.11.2",
    "nodemailer": "^2.5.0",
    "request-prcomise": "^3.0.0",
    "unirest": "^0.4.2"
  },
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-preset-es2015": "^6.9.0",
    "chai": "^3.5.0",
    "eslint": "^3.1.0",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.10.3",
    "eslint-plugin-jsx-a11y": "^2.0.1",
    "eslint-plugin-react": "^5.2.2",
    "istanbul": "^1.1.0-alpha.1",
    "mocha": "^2.5.3",
    "sinon": "^1.17.4"
  },
  "scripts": {
    "test": "NODE_PATH=. node_modules/.bin/mocha --require babel-core/register tests/**.js tests/**/*.js",
    "test:watch": "NODE_PATH=. node_modules/.bin/mocha --require babel-core/register -w tests/**.js tests/**/*.js",
    "coverage": "NODE_PATH=. ./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha -- tests/**.js tests/**/*.js --compilers js:babel-register",
    "isparta": "NODE_PATH=. babel-node ./node_modules/.bin/isparta cover ./node_modules/.bin/_mocha -- tests/**.js tests/**/*.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/magicmark/wafflebot.git"
  },
  "author": "Mark Larah",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/magicmark/wafflebot/issues"
  },
  "homepage": "https://github.com/magicmark/wafflebot"
}
